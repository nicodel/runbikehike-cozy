  },
  "author": "Nicolas Delebecque",
  "license": "MPL2.0 licence",
  "bugs": {
    "url": "https://github.com/nicodel/runbikehike-cozy/issues"
  },
  "cozy-displayName": "Run, Bike, Hike...",
  "cozy-permissions": {
    "docs": {
      "description": "docs contains all the informations"
    }
  },
  "icon-path": "img/icon128.png",
  "dependencies": {
    "americano": "^0.4.2",
    "body-parser": "^1.13.2",
    "cozydb": "0.1.3",
    "express": "^4.13.1",
    "pouchdb": "5.1.0",
    "webl10n": "^1.0.0"
  }
}
